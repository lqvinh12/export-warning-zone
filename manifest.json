{
  "manifest_version": 3,
  "name": "Export Warning Zone",
  "version": "1.0.0",
  "description": "Export data from web portal",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*.daikin.com.vn/*"
  ]
}